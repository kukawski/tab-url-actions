{
	"manifest_version": 2,
	"name": "Tab URL Actions",
	"description": "Quickly navigate to a new URL based on active URL.",
	"version": "0.1",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"background": {
		"page": "background.html",
		"persistent": false
	},
	"browser_action": {
		"default_icon": "icon26.png",
		"default_popup": "popup.html"
	},
	"options_page": "options.html",
	"permissions": [
		"activeTab",
		"storage"
	]
}
